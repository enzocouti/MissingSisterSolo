  m_Script: {fileID: 11500000, guid: 6a8428ae8eb49984b9c433f1784617a3, type: 3}
  m_Name: IntroConversation
  m_EditorClassIdentifier: 
  lines:
  - speakerName: Beatrice
    lineText: '"Hmmm... no... come back..."'
    leftPortrait: {fileID: 0}
    rightPortrait: {fileID: 0}
    highlightSide: 1
  - speakerName: Robert
    lineText: '"Wake up brawler... you are having nightmares again..."'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: 48e93eef0688c4a259cb0eddcd8661f7, type: 3}
    highlightSide: 2
  - speakerName: Beatrice
    lineText: '"Ah... what do you want rob?"'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 1
  - speakerName: Robert
    lineText: '"Got news about your sister."'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Robert
    lineText: '"Something big is happening on that skyscraper..."'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Robert
    lineText: '"But it is filled with those shitty enforcers..."'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Beatrice
    lineText: '"I dont care about enforcers... what should I do?"'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 1
  - speakerName: Robert
    lineText: '"There are some gang hideouts nearby, go kick some ass and spread
      the enforcers around"'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Robert
    lineText: '"..."'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Robert
    lineText: '"I know it has been years and you miss her but... be careful kid"'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 2
  - speakerName: Beatrice
    lineText: '"Will do, thanks old man... im heading out"'
    leftPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    rightPortrait: {fileID: 7482667652216324306, guid: db750325989564987a34393247d883b4, type: 3}
    highlightSide: 1
